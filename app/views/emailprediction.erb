<div class="container">
  <p>Predicted Email for <%= @predict_hash[:name] %> at <%= @predict_hash[:url] %> </p>
  <% if @predict_hash[:single_pattern] %>
    <%= erb :'_single', locals: {pattern: @predict_hash[:patterns][0]} %>
  <% else %>
    <%= erb :'_multiple', locals: {patterns: @predict_hash[:patterns]} %>
  <% end %>
</div>